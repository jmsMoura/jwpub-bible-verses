{
  "name": "obsidian-jwpub-bible-verses",
  "version": "1.0.0",
  "description": "A plugin for Obsidian to insert Bible verses from JW.org",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "bible",
    "jw.org",
    "jwpub"
  ],
  "author": "Thiago Amendola",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.5",
    "obsidian": "^1.4.11",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2"
  }
} 